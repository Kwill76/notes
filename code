name: Demo Workflow
- name: Run command with self-signed certificate
  run: |
    curl --cacert /path/to/self_signed_certificate.pem https://example.com


on:
  push:
    branches:
      - main

jobs:
  build:
    runs-on: windows-latest

    steps:
      - name: Checkout repository
        uses: actions/checkout@v2

      - name: Run script
        run: |
          echo "Hello, world!"
